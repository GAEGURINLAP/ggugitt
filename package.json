{
  "name": "ggugitt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "host": "vite --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "host:dev": "firebase use default && vite --host",
    "host:prd": "firebase use prd && vite -m production --host",
    "deploy:dev": "firebase use default && vite build --mode development && firebase deploy --only hosting:ggugitt-dev",
    "deploy:prd": "firebase use prd && vite build && firebase deploy --only hosting:ggugitt-prd"
  },
  "reactSnap": {
    "include": [
      "/",
      "/vote-result"
    ],
    "staticFileGlobs": [
      "build/**/*"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@sentry/react": "^10.2.0",
    "@sentry/tracing": "^7.120.4",
    "firebase": "^10.1.0",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.14.2",
    "react-snap": "^1.23.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
